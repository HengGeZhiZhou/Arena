<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="priv.lyh.arena.entity.MobileUser" table="mobile_user" schema="project_db">
        <id name="muId" column="mu_id"/>
        <property name="muUId" column="mu_u_id"/>
        <property name="muLongitud" column="mu_longitud"/>
        <property name="muLatitude" column="mu_latitude"/>
    </class>
</hibernate-mapping>